{
  "linters": {
    "python": {
      "type": "black",
      "include": ["(\\.py)"],
      "exclude": [
        "(\\_pb2.py)",
        "(\\_grpc.py)"
      ]
    },
    "buildifier": {
      "type": "buildifier",
      "include": [
        "((^|/)BUILD$)",
        "(\\.bzl$)",
        "(\\.bazel$)",
        "((^|/)WORKSPACE$)"
      ]
    }
  }
}
